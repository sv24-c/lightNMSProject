<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <!--<constant name="struts.devMode" value="true" />-->

    <!--<package name="Start" namespace="/" extends="struts-default">
        
        <interceptors>
            <interceptor name="userSession" class=""></interceptor>
        </interceptors>
        
        <action name="">
            <interceptor-ref name="userSession"/>
            <result>login.jsp</result>
        </action>
    </package>-->

    <package name="default" extends="struts-default">

        <result-types>
            <result-type name="json" class="org.apache.struts2.json.JSONResult">
                <param name="noCache">true</param>
                <param name="excludeNullProperties">true</param>
                <param name="enableGZIP">true</param>
            </result-type>
        </result-types>

        <action name="">
            <result>login.jsp</result>
        </action>
        
        <action name="loginProcess" class="action.LoginAction" method="login">
            <result name="success">/WEB-INF/index.jsp</result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="logoutProcess">
            <result>login.jsp</result>
        </action>

        <action name="dashboardClick" class="action.DashboardAction" method="dashboardData">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>

        </action>

        <action name="discoveryProcess" class="action.DiscoveryAction" method="discovery">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="discoveryInsertProcess" class="action.DiscoveryAction" method="discoveryInsert">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="discoveryGetUsernameProcess" class="action.DiscoveryAction" method="discoveryGetUsername">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="discoveryUpdateProcess" class="action.DiscoveryAction" method="discoveryUpdate">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="discoveryDeleteProcess" class="action.DiscoveryAction" method="discoveryDelete">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="monitorProcess" class="action.MonitorAction" method="monitor">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="monitorShowDataProcess" class="action.MonitorAction" method="showAllData">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="monitorDeleteProcess" class="action.MonitorAction" method="monitorDelete">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

        <action name="monitorShowTableProcess" class="action.MonitorAction" method="monitorShowInChart">
            <result name="success" type="json"></result>
            <result name="failure">login.jsp</result>
        </action>

    </package>

</struts>